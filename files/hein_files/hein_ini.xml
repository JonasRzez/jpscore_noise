<?xml version="1.0" encoding="UTF-8" ?>

<JuPedSim project="scenario_ben" version="0.8"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:noNamespaceSchemaLocation="../xsd/jps_ini_core.xsd">

    <!-- seed used for initialising random generator -->
    <seed>1</seed>
    <!-- geometry file -->
    <max_sim_time>200</max_sim_time>
    <geometry>hein_geo.xml</geometry>
    <!-- traectories file and format -->
    <trajectories format="xml-plain" fps="10">
        <file location="hein_traj.xml" />
    </trajectories>
    <!-- where to store the logs -->
    <showstatistics>true</showstatistics>
    <logfile>log.txt</logfile>



        <!--persons information and distribution -->
    <agents operational_model_id="3">
            <agents_distribution>
                    <group group_id="1" agent_parameter_id="1" room_id="0" subroom_id="1" number="3" router_id="1" pre_movement_mean="0" pre_movements_sigma="0" y_min="0.0" y_max="1.0" />
                    <group group_id="2" agent_parameter_id="1" room_id="0" subroom_id="1" number="3" router_id="1" pre_movement_mean="0" pre_movements_sigma="0" y_min="1.0" y_max="2.0" />
                    <group group_id="1" agent_parameter_id="1" room_id="0" subroom_id="1" number="3" router_id="1" pre_movement_mean="0" pre_movements_sigma="0" y_min="2.0" y_max="3.0" />
                    <group group_id="1" agent_parameter_id="1" room_id="0" subroom_id="1" number="3" router_id="1" pre_movement_mean="0" pre_movements_sigma="0" y_min="3.0" y_max="4.0" />
                    <group group_id="1" agent_parameter_id="1" room_id="0" subroom_id="1" number="3" router_id="1" pre_movement_mean="0" pre_movements_sigma="0" y_min="4.0" y_max="5.0" />
                    <group group_id="1" agent_parameter_id="1" room_id="0" subroom_id="1" number="3" router_id="1" pre_movement_mean="0" pre_movements_sigma="0" y_min="5.0" y_max="6.0" />
                    <group group_id="1" agent_parameter_id="1" room_id="0" subroom_id="1" number="2" router_id="1" pre_movement_mean="0" pre_movements_sigma="0" y_min="6.0" y_max="7.0" />
                    <group group_id="1" agent_parameter_id="1" room_id="0" subroom_id="1" number="30" router_id="1" pre_movement_mean="0" pre_movements_sigma="0" x_min = "-1.5" x_max ="1.5" y_min="6.7" y_max="9.0" />
            </agents_distribution>
    </agents>

            <!-- These parameters may be overwritten -->
    <model operational_model_id="3" description="Trodeux2015">
      <model_parameters>
            <solver>euler</solver>
            <stepsize>0.1</stepsize>
            <periodic>0</periodic>
            <exit_crossing_strategy>3</exit_crossing_strategy>
            <linkedcells enabled="true" cell_size="30" />
            <force_ped a="5" D="0.1" />
            <force_wall a="5" D="0.02" />
      </model_parameters>
      <agent_parameters agent_parameter_id="1">
            <v0 mu="1.34" sigma="0.0" />
            <bmax mu="0.175" sigma="0.0" />
            <bmin mu="0.175" sigma="0.0" />
            <amin mu="0.175" sigma="0.0" />
            <tau mu="0.5" sigma="0.0" />
            <atau mu="1." sigma="0.0" />
      </agent_parameters>
    </model>


    <route_choice_models>
            <router router_id="1" description="ff_global_shortest" />
    </route_choice_models>

</JuPedSim>
